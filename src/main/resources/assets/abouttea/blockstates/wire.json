{
  "multipart": [
    {
      "apply": {
        "model": "abouttea:block/wire_core"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "abouttea:block/wire_part",
        "y": 90
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "abouttea:block/wire_part",
        "y": 180
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "abouttea:block/wire_part",
        "y": 270
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "abouttea:block/wire_part"

      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "abouttea:block/wire_part",
        "z": 270
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "abouttea:block/wire_part",
        "z": 90
      }
    }
  ]
}